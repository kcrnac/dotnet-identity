{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Yarp": "Information",
      "Microsoft": "Information"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "identity": {
        "Destinations": {
          "accountApi": {
            "Address": "http://identity-svc"
          }
        }
      },
      "webApp": {
        "Destinations": {
          "web": {
            "Address": "http://web-app"
          }
        }
      }
    }
  }
}
