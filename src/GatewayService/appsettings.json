{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "Identity": "http://localhost:5000"
  },
  "ReverseProxy": {
    "Routes": {
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/identity/{**catch-all}",
          "Methods": ["POST", "GET"]
        },
        "Transforms": [
          {
            "PathPattern": "api/identity/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "accountApi": {
            "Address": "http://localhost:5000"
          }
        }
      }
    }
  }
}
